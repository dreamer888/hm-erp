<?xml version="1.0"?>
<odoo>
    <record id="goods_reference_black_list_tree" model="ir.ui.view">
        <field name="name">goods.reference.black.list.tree</field>
        <field name="model">goods.reference.black.list</field>
        <field name="arch" type="xml">
            <tree string='商品引用检测黑名单' editable='bottom'>
                <field name='ref_model_id' required='1'/>
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="goods_reference_black_list_action">
        <field name="name">商品引用检测黑名单</field>
        <field name="res_model">goods.reference.black.list</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">点击创建添加黑名单</p>
        </field>
    </record>
</odoo>