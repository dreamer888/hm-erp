<?xml version='1.0' encoding='utf-8'?>
<openerp>
    <data noupdate='1'>
        <record id='sequence_miscellaneous_journal' model='ir.sequence'>
            <field name='name'>Account Default Miscellaneous Journal</field>
            <field eval='3' name='padding'/>
            <field name='prefix'>MISJ/%(year)s/</field>
        </record>

        <record id='wh_out_sequence' model='ir.sequence'>
            <field name='name'>出库单</field>
            <field name='code'>wh.out</field>
            <field eval='4' name='padding'/>
            <field name='prefix'>OTHERS/OUT/%(y)s%(month)s</field>
        </record>

        <record id='wh_in_sequence' model='ir.sequence'>
            <field name='name'>入库单</field>
            <field name='code'>wh.in</field>
            <field eval='4' name='padding'/>
            <field name='prefix'>OTHERS/IN/%(y)s%(month)s</field>
        </record>

        <record id='wh_internal_sequence' model='ir.sequence'>
            <field name='name'>移库单</field>
            <field name='code'>wh.internal</field>
            <field eval='4' name='padding'/>
            <field name='prefix'>TRANSFER/%(y)s%(month)s</field>
        </record>

        <record id='wh_inventory_sequence' model='ir.sequence'>
            <field name='name'>盘点单</field>
            <field name='code'>wh.inventory</field>
            <field eval='4' name='padding'/>
            <field name='prefix'>INV/%(y)s%(month)s</field>
        </record>

        <record id='wh_assembly_sequence' model='ir.sequence'>
            <field name='name'>组装单</field>
            <field name='code'>wh.assembly</field>
            <field eval='4' name='padding'/>
            <field name='prefix'>ASS/%(y)s%(month)s</field>
        </record>

        <record id='wh_disassembly_sequence' model='ir.sequence'>
            <field name='name'>拆卸单</field>
            <field name='code'>wh.disassembly</field>
            <field eval='4' name='padding'/>
            <field name='prefix'>DIS/%(y)s%(month)s</field>
        </record>

        <record id='outsource_sequence' model='ir.sequence'>
            <field name='name'>委外加工单</field>
            <field name='code'>outsource</field>
            <field eval='4' name='padding'/>
            <field name='prefix'>OUT/%(y)s%(month)s</field>
        </record>
    </data>
</openerp>
