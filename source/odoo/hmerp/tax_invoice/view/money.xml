<?xml version="1.0"?>
<odoo>

	<record id="money_invoice_tree_inherit" model="ir.ui.view">
		<field name='name'>money.invoice.tree.inherit</field>
		<field name="model">money.invoice</field>
		<field name="inherit_id" ref="money.money_invoice_tree"/>
		<field name="arch" type="xml">
			<button name="money_invoice_done" position="replace">
			    <field name="tax_invoice_id" invisible="1"/>
				<button name="money_invoice_done" string="чбошод" type="object"
				        attrs="{'invisible':['|',('tax_invoice_id','!=',False),('state','=','done')]}"/>
			</button>
		</field>
	</record>

</odoo>
