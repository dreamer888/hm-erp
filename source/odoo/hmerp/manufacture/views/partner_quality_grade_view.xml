<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id='partner_quality_grade_tree_view' model='ir.ui.view'>
        <field name='name'>供应商品质等级</field>
        <field name='model'>partner.quality.grade</field>
        <field name='arch' type='xml'>
            <tree editable='bottom'>
                <field name='name' />
                <field name='grade' />
                <field name='remark' />
            </tree>
        </field>
    </record>
    <record id="partner_quality_grade_action" model="ir.actions.act_window">
            <field name="name">供应商品质等级</field>
            <field name="res_model">partner.quality.grade</field>
            <field name="view_id" ref='partner_quality_grade_tree_view'/>
            <field name="view_mode">tree</field>
    </record>
    <record id="partner_quality_grade_search" model="ir.ui.view">
        <field name="name">partner.quality.grade.search</field>
        <field name="model">partner.quality.grade</field>
        <field name="arch" type="xml">
            <search string='供应商品质等级搜索视图'>
                <field name="name" />
                <field name='remark'/>
            </search>
        </field>
    </record>
</odoo>