<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_form_goods_goods_extend" model="ir.ui.view">
        <field name="name">Goods Extened</field>
        <field name="model">goods</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="goods.goods_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name='mrp_info' string="生产">
                    <group>
                        <group>
                            <field name="mrp_qc"/>
                            <field name="department_id"/>
                            <field name="out_warehouse_id"/>
                            <field name="in_warehouse_id"/>
                            <field name="pre_days"/>
                        </group>
                    </group>
                </page>
            </xpath>
            <xpath expr="//field[@name='not_buyable']" position="before">
                <field name="buy_qc"/>
            </xpath>
        </field>
    </record>
</odoo>