<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="mrp_group" model="res.groups">
        <field name="name">生产人员</field>
        <field name="implied_ids" eval="[4,(ref('base.group_user'))]"/>
    </record>
    <record id="mrp_category_manager" model="res.groups">
        <field name="name">生产主管</field>
        <field name="implied_ids" eval="[(4,ref('mrp_group'))]"/>
        <field name="users" eval="[
            (4,ref('base.user_root')),
            (4,ref('base.user_admin'))
        ]"/>
    </record>
</odoo>