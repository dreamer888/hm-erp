<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

       <!-- Inherit Form View to Modify it -->
       <record id="res_company_invisible_form_inherit" model="ir.ui.view">
           <field name="name">res.company.invisible.staff</field>
           <field name="model">res.company</field>
           <field name="inherit_id" ref="staff.staff_company_form_inherit"/>
           <field name="arch" type="xml">

               <xpath expr="//notebook/page[3]" position="attributes">
                   <attribute name="attrs">{'invisible': 1}</attribute>
               </xpath>

           </field>
       </record>

    </data>
</odoo>