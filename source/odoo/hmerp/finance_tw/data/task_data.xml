<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

       <record id="task.task_status_todo" model="task.status">
			<field name="name">未開始</field>
			<field name="state">todo</field>
			<field name="sequence">1</field>
		</record>
		<record id="task.task_status_done" model="task.status">
			<field name="name">已完成</field>
			<field name="state">done</field>
			<field name="sequence">100</field>
		</record>
		<record id="task.task_status_cancel" model="task.status">
			<field name="name">已取消</field>
			<field name="state">cancel</field>
			<field name="sequence">999</field>
		</record>

    </data>
</odoo>