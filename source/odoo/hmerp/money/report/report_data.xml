        <!--对账单打印-->
        <record id='customer_overdue_report' model='ir.actions.report'>
            <field name='name'>对账单</field>
            <field name='model'>partner</field>
            <field name='binding_model_id' ref='core.model_partner'/>
            <field name='report_type'>docx</field>
            <field name='report_name'>customer.overdue.report</field>
            <field name='output_type'>pdf</field>
            <field name='template_file'>money/template/customer.overdue.report.docx</field>
        </record>
