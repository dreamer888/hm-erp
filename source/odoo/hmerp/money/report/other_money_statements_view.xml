            <field name="name">other.money.statements.report.tree</field>
            <field name="model">other.money.statements.report</field>
            <field name="arch" type="xml">
                <tree string="其他收支明细表" create='0'>
                	<field name="date" optional="show"/>
                    <field name="name" optional="show"/>
                    <field name="note" optional="show"/>
                    <field name="partner_id" optional="show"/>
                    <field name="bank_id" optional="show"/>
                    <field name="service" optional="show"/>
                    <field name="category_id" optional="show"/>
                    <field name="get" sum="合计" optional="show"/>
                    <field name="pay" sum="合计" optional="show"/>
                </tree>
            </field>
        </record>
	</data>
</openerp>
	